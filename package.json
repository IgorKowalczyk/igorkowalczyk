{
  "name": "@igorkowalczyk/igorkowalczyk",
  "version": "3.1.0",
  "description": "A script to auto-update my profile readme with latest statisics",
  "exports": "src/index.js",
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/igorkowalczyk/igorkowalczyk"
  },
  "bugs": {
    "url": "https://github.com/igorkowalczyk/igorkowalczyk/issues"
  },
  "scripts": {
    "build": "node src/index.js",
    "format": "prettier --write . --ignore-unknown --cache",
    "format:check": "prettier --check . --ignore-unknown --cache",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix"
  },
  "dependencies": {
    "@igorkowalczyk/eslint-config": "1.4.2",
    "@igorkowalczyk/prettier-config": "1.4.2",
    "actions-toolkit": "6.0.1",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "graphql": "16.7.1",
    "graphql-request": "6.1.0",
    "markdown-table": "3.0.3",
    "node-fetch": "3.3.1",
    "xml2js": "0.6.0"
  },
  "devDependencies": {
    "eslint": "8.43.0",
    "prettier": "2.8.8"
  },
  "packageManager": "pnpm@8.6.5"
}
